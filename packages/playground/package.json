{
  "name": "@ytspar/playground",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "description": "Development playground for testing DevBar and Sweetlink",
  "scripts": {
    "dev": "concurrently -n vite,sweetlink -c green,blue \"vite\" \"SWEETLINK_WS_PORT=11396 PORT=5173 node ../sweetlink/dist/cli/sweetlink-dev.js\"",
