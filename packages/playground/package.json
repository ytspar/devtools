    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@ytspar/devbar": "workspace:*",
    "@ytspar/sweetlink": "workspace:*"
  },
  "devDependencies": {
    "concurrently": "^9.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.0.0"
  }
}
